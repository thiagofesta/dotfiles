{
  // "diffEditor.renderSideBySide": false,
  // "editor.fontFamily": "Source Code Pro",
  "editor.fontSize": 13,
  // "editor.minimap.enabled": true,
  // "editor.minimap.maxColumn": 80,
  // "editor.rulers": [79],
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  // "explorer.confirmDragAndDrop": false,
  // "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 0,
  // "explorer.autoReveal": false,
  "files.autoSave": "onWindowChange",
  "files.associations": {
    "*.tl": "dustjs"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.gradle": true,
    "**/*.a": true,
    "**/*.class": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.exe": true,
    "**/*.eyeglass_cache": true,
    "**/*.fabric": true,
    "**/*.git": true,
    "**/*.gitkeep": true,
    "**/*.hg": true,
    "**/*.idb": true,
    "**/*.jar": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.lib": true,
    "**/*.map": true,
    "**/*.ncb": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.src": true,
    "**/*.suo": true,
    "**/*.svn": true,
    "**/bower_components": true,
    "**/build": true,
    "**/CVS": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.useExperimentalFileWatcher": true,
  "search.exclude": {
    "**/.DS_Store": true,
    "**/*.a": true,
    "**/*.class": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.exe": true,
    "**/*.eyeglass_cache": true,
    "**/*.fabric": true,
    "**/*.git": true,
    "**/*.gitkeep": true,
    "**/.gradle": true,
    "**/*.hg": true,
    "**/*.idb": true,
    "**/*.jar": true,
    "**/*.js": {"when": "$(basename).ts"},
    "**/*.lib": true,
    "**/*.map": true,
    "**/*.ncb": true,
    "**/*.obj": true,
    "**/*.o": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.src": true,
    "**/*.suo": true,
    "**/*.svn": true,
    "**/CVS": true,
    "**/bower_components": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  // "typescript.check.npmIsInstalled": false
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  // "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Darcula Theme from IntelliJ",
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "none",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  // Ember related files plugin
  "emberRelatedFiles.showQuickPickForSingleOption": true
}
